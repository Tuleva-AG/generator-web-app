{
  "name": "@tulevaag/generator-web-app",
  "version": "0.0.5",
  "description": "Generator for standarized web app using react, webpack and other up to date tools",
  "homepage": "https://github.com/Tuleva-AG/generator-web-app",
  "author": {
    "name": "Sebastian Sch√ºtze",
    "email": "sebastian.schuetze@tuleva.de",
    "url": "http://www.tuleva.de"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "files": [
    "generators",
    "common"
  ],
  "main": "generators/index.js",
  "keywords": [
    "generator",
    "tuleva",
    "project template",
    "webapp",
    "yeoman-generator"
  ],
  "devDependencies": {
    "eslint": "^5.15.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-xo": "^0.26.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "lint-staged": "^8.1.5",
    "nsp": "^3.2.1",
    "prettier": "^1.16.4",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^1.9.1"
  },
  "dependencies": {
    "builtins": "^2.0.0",
    "chalk": "^2.4.2",
    "yeoman-generator": "^3.2.0",
    "yosay": "^2.0.2"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "prepublish": "npm audit --audit-level high",
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "repository": "https://github.com/Tuleva-AG/generator-web-app",
  "private": false
}
