{
  "name": "@tulevaag/generator-web-app",
  "version": "0.0.5",
  "description": "Generator for standarized web app using react, webpack and other up to date tools",
  "homepage": "https://github.com/Tuleva-AG/generator-web-app",
  "author": {
    "name": "Sebastian Sch√ºtze",
    "email": "sebastian.schuetze@tuleva.de",
    "url": "http://www.tuleva.de"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "files": [
    "generators",
    "common"
  ],
  "main": "generators/index.js",
  "keywords": [
    "generator",
    "tuleva",
    "project template",
    "webapp",
    "yeoman-generator"
  ],
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-xo": "^0.20.1",
    "eslint-plugin-prettier": "^2.6.1",
    "husky": "^0.14.3",
    "jest": "^22.4.4",
    "lint-staged": "^7.2.0",
    "nsp": "^3.2.1",
    "prettier": "^1.13.7",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^1.8.0"
  },
  "dependencies": {
    "builtins": "^2.0.0",
    "chalk": "^2.4.1",
    "yeoman-generator": "^2.0.5",
    "yosay": "^2.0.2"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "prepublish": "nsp check",
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "repository": "https://github.com/Tuleva-AG/generator-web-app",
  "private": false
}
